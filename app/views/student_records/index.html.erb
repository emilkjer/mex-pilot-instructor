<h1>Listing student_records</h1>

<table>
  <tr>
    <th>Present</th>
    <th>Preparation</th>
    <th>Personalattitude</th>
    <th>Note</th>
    <th>Student student</th>
    <th>Classdate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @student_records.each do |student_record| %>
  <tr>
    <td><%= student_record.present %></td>
    <td><%= student_record.preparation %></td>
    <td><%= student_record.personalAttitude %></td>
    <td><%= student_record.note %></td>
    <td><%= student_record.student_student_id %></td>
    <td><%= student_record.classDate %></td>
    <td><%= link_to 'Show', student_record %></td>
    <td><%= link_to 'Edit', edit_student_record_path(student_record) %></td>
    <td><%= link_to 'Destroy', student_record, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

